{
  "name": "morcus-net",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "format": "npx prettier . --write && python -m black .",
    "format-check": "npx prettier . --check && python -m black . --check",
    "setup-alatius": "python src/scripts/alatius.py",
    "test": "npm run ts-tests && npm run py-tests",
    "ts-tests": "npx jest",
    "py-tests": "python -m unittest discover",
    "py-coverage": "python -m coverage run -m unittest discover && coverage xml --omit **/test_*.py -o coverage/coverage.xml",
    "ts-coverage": "npx jest --coverage",
    "coverage": "npm run ts-coverage && npm run py-coverage",
    "ts-node": "ts-node -r tsconfig-paths/register",
    "pytype": "python -m pytype src/py src/scripts",
    "build-client": "npx webpack",
    "server-w": "npm run build-client && npm run ts-node src/driver.ts",
    "server-p": "npm run ts-node src/web/nlp/processing_server.ts",
    "driver": "npm run ts-node src/driver.ts",
    "py-driver": "python -c \"import src.py.driver\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nkprasad12/morcus-net.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nkprasad12/morcus-net/issues"
  },
  "homepage": "https://github.com/nkprasad12/morcus-net#readme",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-static-gzip": "^2.1.7",
    "fast-xml-parser": "^4.1.2",
    "socket.io": "^4.6.1",
    "ts-lib": "^0.0.5",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^10.0.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.4.2",
    "prettier": "2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io-client": "^4.6.1",
    "terser-webpack-plugin": "^5.3.6",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
